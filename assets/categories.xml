<?xml version="1.0" encoding="utf-8"?>
<categories xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="categories.xsd">
	<custom name="custom_search"/>
	<starred name="starred"/>
	<category name="emergency" >
		<search name="hospital" query="amenity=hospital"/>
		<search name="clinic" query="amenity=clinic"/>
		<search name="doctors" query="amenity=doctors"/>
		<search name="ambulance_station" query="emergency=ambulance_station" />
		<search name="emergency_phone" query="emergency=phone"/>
		<search name="emergency_bay" query="highway=emergency_bay" />
		<search name="police" query="amenity=police" />
	</category>
	<category name="transport">
		<search name="bus_stop" query="highway=bus_stop"/>
		<search name="bus_station" query="amenity=bus_station"/>
		<search name="railway_station" query="railway=station | building=train_station"/>
		<search name="airport" query="aeroway=aerodrome" />
		<search name="parking" query="amenity=parking" />
		<search name="fuel" query="amenity=fuel" />
		<inline name="fuel_operator" query="amenity=fuel" select="operator*" />
		<search name="car_wash" query="amenity=car_wash" />
	</category>
	<category name="finance">
		<search name="bank" query="amenity=bank"/>
		<search name="atm" query="amenity=atm | atm=yes"/>
	</category>
	<category name="shopping">
		<search name="all_in_this_category" query="shop=*" />
		<inline name="supermarket" query="shop=supermarket" select="operator*"/>
		<search name="mall" query="shop=mall"/>
		<search name="convenience" query="shop=convenience" />
		<inline name="shop_operator" query="shop=*" select="operator*" />
	</category>
	<category name="services">
		<search name="toilet" query="amenity=toilets" />
		<search name="telephone" query="amenity=telephone" />
		<search name="post_box" query="amenity=post_box" />
		<search name="post_office" query="amenity=post_office" />
	</category>
	<category name="food">
		<search name="cafe" query="amenity=cafe" />
		<search name="restaurant" query="amenity=restaurant" />
		<search name="fast_food" query="amenity=fast_food" />
		<search name="pub" query="amenity=pub" />
	</category>
</categories>